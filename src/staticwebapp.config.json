{
    "routes": [
      {
        "route": "/*",
        "allowedRoles": ["authenticated"]
      }
    ],
    "responseOverrides": {
      "401": {
        "statusCode": 302,
        "redirect": "/.auth/login/guide"
      }
    },
    "auth": {
        "identityProviders": {
          "customOpenIdConnectProviders": {
            "guide": {
              "registration": {
                "clientIdSettingName": "LOCAL_GUIDE_CLIENT_ID",
                "clientCredential": {
                  "clientSecretSettingName": "LOCAL_GUIDE_CLIENT_SECRET"
                },
                "openIdConnectConfiguration": {
                  "wellKnownOpenIdConfiguration": "https://localhost:44373/oauthserver/openidconfig"
                }
              },
              "login": {
                "nameClaimType": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
                "scopes": [],
                "loginParameterNames": []
              }
            }
          }
        }
      }
  }