{
    "routes": [
      {
        "route": "/*",
        "allowedRoles": ["authenticated"]
      }
    ],
    "responseOverrides": {
      "401": {
        "statusCode": 302,
        "redirect": "/.auth/login/aadb2c"
      }
    },
    "auth": {
        "identityProviders": {
          "customOpenIdConnectProviders": {
            "aadb2c": {
              "registration": {
                "clientIdSettingName": "AADB2C_PROVIDER_CLIENT_ID",
                "clientCredential": {
                  "clientSecretSettingName": "AADB2C_PROVIDER_CLIENT_SECRET"
                },
                "openIdConnectConfiguration": {
                  "wellKnownOpenIdConfiguration": "https://someb2corganisation.b2clogin.com/someb2corganisation.onmicrosoft.com/B2C_1_Test_1/v2.0/.well-known/openid-configuration"
                }
              },
              "login": {
                "nameClaimType": "emails",
                "scopes": ["openid"],
                "loginParameterNames": []
              }
            }
          }
        }
    }
  }